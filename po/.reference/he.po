#. Various error messages we may want to print
#: sed/compile.c:140
#, c-format
msgid "bad regexp: %s\n"
msgstr "%s ירלוגר יוטיבב האיגש\n"

#: sed/compile.c:141
msgid "Invalid use of address modifier"
msgstr "תבותכה ןייצמב יוגש שומיש"

#: sed/compile.c:142
msgid "Multiple `!'s"
msgstr "`!' יוביר"

#: sed/compile.c:143
msgid "Unexpected `,'"
msgstr "יופצ-יתלב `,'"

#: sed/compile.c:144
msgid "Unexpected End-of-file"
msgstr "ידמ םדקומ םייתסמ ץבוק"

#: sed/compile.c:145
msgid "Cannot use +N or ~N as first address"
msgstr "הנושאר תבותככ ~N וא +N-ב שמתשהל ןתינ אל"

#: sed/compile.c:146
msgid "Unmatched `{'"
msgstr "גוז-ןב ול ןיאש `{'"

#: sed/compile.c:147
msgid "Unexpected `}'"
msgstr "יופצ-יתלב `}'"

#: sed/compile.c:148
msgid "Extra characters after command"
msgstr "הדוקפה ירחא םירתוימ םיות"

#: sed/compile.c:149
msgid "`}' doesn't want any addresses"
msgstr "תובותכ לבקמ וניא `}'"

#: sed/compile.c:150
msgid ": doesn't want any addresses"
msgstr "תובותכ לבקמ וניא :"

#: sed/compile.c:151
msgid "Comments don't accept any addresses"
msgstr "תובותכ תולבקמ ןניא תורעה"

#: sed/compile.c:152
msgid "Missing command"
msgstr "הרסח הדוקפ"

#: sed/compile.c:153
msgid "Command only uses one address"
msgstr "דבלב תחא תבותכ תלבקמ וז הדוקפ"

#: sed/compile.c:154
msgid "Unterminated address regex"
msgstr "םויס אלל תבותכ לש ירלוגר יוטיב"

#: sed/compile.c:155
msgid "Unterminated `s' command"
msgstr "םויס אלל `s' תדוקפ"

#: sed/compile.c:156
msgid "Unterminated `y' command"
msgstr "םויס אלל `y' תדוקפ"

#: sed/compile.c:157
msgid "Unknown option to `s'"
msgstr "`s' לש רכומ-יתלב ןייפאמ"

#: sed/compile.c:158
msgid "multiple `p' options to `s' command"
msgstr "`s' הדוקפל םיבורמ `p' ינייפאמ"

#: sed/compile.c:159
msgid "multiple `g' options to `s' command"
msgstr "`s' הדוקפל םיבורמ `g' ינייפאמ"

#: sed/compile.c:161
msgid "multiple number options to `s' command"
msgstr "`s' הדוקפל םיבורמ רפסמ ינייפאמ"

#: sed/compile.c:163
msgid "number option to `s' command may not be zero"
msgstr "ספא תויהל לוכי וניא `s' הדוקפל ירפסמ ןייפאמ"

#: sed/compile.c:165
msgid "strings for y command are different lengths"
msgstr "הנוש ךרואב ןניה `y' הדוקפל תוזורחמ"

#: sed/compile.c:207
#, c-format
msgid "Called savchar() with unexpected pushback (%x)"
msgstr "(%x) יופצ-יתלב pushback םע הארקנ savchar() היצקנופ"

#: sed/compile.c:1049
msgid "Unknown command:"
msgstr "תרכומ-יתלב הדוקפ הניה"

#: sed/compile.c:1070
#, c-format
msgid "%s: file %s line %lu: %s\n"
msgstr "%s תינכתב (%s ץבוק לש %lu הרוש) %s האיגש\n"

#: sed/compile.c:1073
#, c-format
msgid "%s: -e expression #%lu, char %lu: %s\n"
msgstr "%s תינכתב (%lu 'סמ -e יוטיב לש %lu 'סמ ות) %s האיגש\n"

#: sed/compile.c:1251
#, c-format
msgid "Can't find label for jump to `%s'"
msgstr "האצמנ אל `%s' הציפק תיוות"

#: sed/execute.c:331
#, c-format
msgid "input read error: %s"
msgstr "האירק תעב (%s) האיגש"

#: sed/execute.c:480
#, c-format
msgid "%s: can't read %s: %s\n"
msgstr "%s תינכתב %s תאירקב (%s) האיגש\n"

#: sed/execute.c:641
msgid "INTERNAL ERROR: bad address type"
msgstr "עודי-יתלב תבותכ גוס :הרומח הנכת תלקת"

#: sed/execute.c:1071
#, c-format
msgid "INTERNAL ERROR: Bad cmd %c"
msgstr "%c תשבושמ הדוקפ :הרומח הנכת תלקת"

#: sed/sed.c:97
#, c-format
msgid ""
"Usage: %s [OPTION]... {script-only-if-no-other-script} [input-file]...\n"
"\n"
"  -n, --quiet, --silent\n"
"                 suppress automatic printing of pattern space\n"
"  -e script, --expression=script\n"
"                 add the script to the commands to be executed\n"
"  -f script-file, --file=script-file\n"
"                 add the contents of script-file to the commands to be executed\n"
"  -l N, --line-length=N\n"
"                 specify the desired line-wrap length for the `l' command\n"
"  -u, --unbuffered\n"
"\n"
"      --help     display this help and exit\n"
"  -V, --version  output version information and exit\n"
"\n"
"If no -e, --expression, -f, or --file option is given, then the first\n"
"non-option argument is taken as the sed script to interpret.  All\n"
"remaining arguments are names of input files; if no input files are\n"
"specified, then the standard input is read.\n"
"\n"
msgstr ""
"%s [םינייפאמ]... {םירחא-םיטירסת-רדעיהב-טירסת} [טלק ץבוק]... :שומיש ןפוא\n"
"\n"
"                                                  -n, --quiet, --silent\n"
"                  יטמוטוא ןפואב תוינבתה בחרמ תא סיפדת לא\n"
"                                         -e script, --expression=script\n"
"                        עצבל שי ןתוא תודוקפל script ףסוה\n"
"                                     -f script-file, --file=script-file\n"
"          עצבל שי ןתוא תודוקפל script-file לש ןכותה ףסוה\n"
"                                                  -l N, --line-length=N\n"
"                       תדוקפ רובע יברימ הרוש לדוגכ N עבק\n"
"                                                       -u, --unbuffered\n"
"\n"
"                                תינכתהמ אצו וז הכרדה גצה         --help\n"
"                                     אצו תימעתה תסרג גצה  -V, --version\n"
"\n"
"       ןושארה טנמוגראה ,--file וא ,-f ,--expression ,-e םינייפאמ רדעיהב\n"
"       םניה םירתונה םיטנמוגראה לכ .עוציבל sed תירסתכ שרפתי ןייפאמ וניאש\n"
"                .ינקת טלק קיפא ארקיי ,טלק יצבק לכ רדעיהב ;טלק יצבק תומש\n"
"\n"

#: sed/sed.c:118
#, c-format
